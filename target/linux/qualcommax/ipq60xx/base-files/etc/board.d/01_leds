
. /lib/functions/uci-defaults.sh

board_config_update

board=$(board_name)

case "$board" in
    jdc,ax1800-pro)
        ucidef_set_led_netdev "wan" "WAN" "net_blue" "eth3"
        ;;
xiaomi,ax1800)
	ucidef_set_led_netdev "internet" "Internet" "blue:network" "eth3"
	;;
*)
	;;
esac

board_config_flush

exit 0
